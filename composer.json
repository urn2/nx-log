{
  "name": "veasin/nx-log",
  "description": "log for nx",
  "homepage": "https://github.com/veasin/nx",
  "type": "framework",
  "authors": [
    {
      "name": "Vea",
      "homepage": "https://www.uper.net",
      "email": "vea@live.cn"
    }
  ],
  "require": {
    "php": ">=8.2",
    "psr/log": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "nx\\parts\\": [
        "src/parts/"
      ],
      "nx\\helpers\\": [
        "src/helpers/"
      ]
    }
  },
  "minimum-stability":"dev",
  "repositories":[
    {
      "type":"path",
      "url":"/codes/me/nx-container/",
      "options":{
        "versions":{
          "veasin/nx-container":"0.0.1-dev"
        }
      }
    },
    {
      "type":"path",
      "url":"/codes/me/nx-io/",
      "options":{
        "versions":{
          "veasin/nx-io":"0.0.1-dev"
        }
      }
    },
    {
      "type":"path",
      "url":"/codes/me/nx/",
      "options":{
        "versions":{
          "veasin/nx":"0.0.1-dev"
        }
      }
    }
  ],
  "require-dev": {
    "veasin/nx": "*"
  }
}
